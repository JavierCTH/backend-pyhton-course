   "source": [
    "import time\n",
    "\n",
    "def countup(time_sec):\n",
    "    while time_sec:\n",
    "        mins, secs = divmod(time_sec, 60)\n",
    "        timeformat = '{:02d}:{:02d}'.format(mins, secs)\n",
    "        print(timeformat, end='\\r')\n",
    "        time.sleep(1)\n",
    "        time_sec += 1\n",
    "    \n",
    "    print(\"stop\")\n",
    "\n",
    "countup(1)"
   ]
  }
